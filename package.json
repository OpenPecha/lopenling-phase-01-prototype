    "@remix-run/serve": "^1.7.6",
    "@sentry/remix": "^7.23.0",
    "@tiptap/extension-document": "^2.0.0-beta.202",
    "@tiptap/extension-highlight": "^2.0.0-beta.202",
    "@tiptap/extension-paragraph": "^2.0.0-beta.202",
    "@tiptap/extension-text": "^2.0.0-beta.202",
    "@tiptap/react": "^2.0.0-beta.202",
    "axios": "^1.2.0",
    "crypto-js": "^4.1.1",
    "discourse-sso": "^1.0.5",
    "isbot": "^3.6.5",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.4.4",
    "remix-auth": "^3.3.0",
    "run-p": "^0.0.0",
    "serve": "^14.1.2",
    "utf8": "^3.0.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.7.6",
    "@remix-run/eslint-config": "^1.7.6",
    "@types/lodash": "^4.14.191",
    "@types/react": "^18.0.25",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "esbuild-register": "^3.4.1",
    "eslint": "^8.27.0",
    "postcss": "^8.4.19",
    "prisma": "^4.6.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=14"
  }
